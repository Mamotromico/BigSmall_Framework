<Patch>
  <!-- These patches are probably not going to be terribly fast...-->
  <Operation Class="BigAndSmall.PatchOp_FantasyEnabled">
    <match Class="BigAndSmall.PatchOp_ReplaceText">
      <xpath>Defs/*/*[self::label or self::description]</xpath>
      <success>Always</success>
      <oldText>anomalous</oldText>
      <newText>soul</newText>
    </match>
  </Operation>
  <Operation Class="BigAndSmall.PatchOp_FantasyEnabled">
    <match Class="BigAndSmall.PatchOp_ReplaceText">
      <xpath>Defs/GeneDef/customEffectDescriptions/li</xpath>
      <success>Always</success>
      <oldText>anomalous</oldText>
      <newText>soul</newText>
    </match>
  </Operation>

  <Operation Class="BigAndSmall.PatchOp_FantasyEnabled">
    <match Class="BigAndSmall.PatchOp_ReplaceText">
      <xpath>//*[self::label or self::description or self::rulesStrings]</xpath>
