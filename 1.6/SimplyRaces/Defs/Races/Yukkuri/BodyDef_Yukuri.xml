<Defs>
  <BodyDef>
    <defName>BS_JustAHead</defName>
    <label>disembodied head</label>
    <corePart>
      <def>Torso</def>
      <customLabel>neck</customLabel>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
        <li>UpperHead</li>
        <li>FullHead</li>
        <li>Neck</li>
      </groups>
      <parts>
        <li>
          <def>Neck</def>
          <customLabel>nape</customLabel>
          <coverage>0.69</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
              <coverage>0.9999</coverage>
              <height>Top</height>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Skull</def>
                  <coverage>0.18</coverage>
                  <depth>Inside</depth>
                  <groups>
                    <li>UpperHead</li>
                    <li>Eyes</li>
                    <li>FullHead</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Brain</def>
                      <coverage>0.8</coverage>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <li>
                      <def>Heart</def>
                      <coverage>0.020</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <li>
                      <def>Kidney</def>
                      <customLabel>left kidney</customLabel>
                      <coverage>0.017</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <li>
                      <def>Kidney</def>
                      <customLabel>right kidney</customLabel>
                      <coverage>0.017</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <li>
                      <def>Liver</def>
                      <coverage>0.025</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>UpperHead</li>
                        <li>FullHead</li>
                        <li>Eyes</li>
                      </groups>
                    </li>
                  </parts>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>FullHead</li>
                    <li>Eyes</li>
                  </groups>
                  <woundAnchorTag>LeftEye</woundAnchorTag>
                  <flipGraphic>true</flipGraphic>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>FullHead</li>
                    <li>Eyes</li>
                  </groups>
                  <woundAnchorTag>RightEye</woundAnchorTag>
                </li>
                <li>
                  <def>Ear</def>
                  <customLabel>left ear</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                  <flipGraphic>true</flipGraphic>
                </li>
                <li>
                  <def>Ear</def>
                  <customLabel>right ear</customLabel>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Nose</def>
                  <coverage>0.10</coverage>
                  <groups>
                    <li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Jaw</def>
                  <coverage>0.15</coverage>
                  <groups>
                    <li>Teeth</li>
                    <li>FullHead</li>
                    <li>Mouth</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Tongue</def>
                      <coverage>0</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>FullHead</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Waist</def>
          <coverage>0</coverage>
          <height>Bottom</height>
          <groups>
            <li>Waist</li>
            <!-- <li>Shoulders</li>
            <li>Arms</li>
            <li>Hands</li>
            <li>RightHand</li>
            <li>Legs</li>
            <li>Feet</li> -->
          </groups>
        </li>
        <!-- I guess it just sits on its stomach? -->
        <li>
          <def>Stomach</def>
          <coverage>0.04</coverage>
          <height>Middle</height>
          <depth>Inside</depth>
          <groups>
            <li>UpperHead</li>
            <li>FullHead</li>
          </groups>
        </li>
        <!-- It, uh, hops around on its... lungs? O_o -->
        <li>
          <def>BS_LocomotiveLung</def>
          <customLabel>left locomotive lung</customLabel>
          <coverage>0.13</coverage>
          <height>Bottom</height>
          <depth>Inside</depth>
          <groups>
            <li>UpperHead</li>
            <li>FullHead</li>
            <li>Shoulders</li>
            <li>Arms</li>
            <li>Hands</li>
            <li>LeftHand</li>
            <li>Legs</li>
            <li>Feet</li>
          </groups>
          <flipGraphic>true</flipGraphic>
        </li>
        <li>
          <def>BS_LocomotiveLung</def>
          <customLabel>right locomotive lung</customLabel>
          <coverage>0.13</coverage>
          <height>Bottom</height>
          <depth>Inside</depth>
          <groups>
            <li>UpperHead</li>
            <li>FullHead</li>
            <li>Shoulders</li>
            <li>Arms</li>
            <li>Hands</li>
            <li>RightHand</li>
            <li>Legs</li>
            <li>Feet</li>
          </groups>
        </li>
        
        <!-- <li>
          <def>Arm</def>
          <customLabel>left splenius</customLabel>
          <coverage>0.000001</coverage>
          <woundAnchorTag>LeftShoulder</woundAnchorTag>
          <groups>
            <li>Shoulders</li>
            <li>Arms</li>
            <li>Hands</li>
          </groups>
          <flipGraphic>true</flipGraphic>
        </li>
        <li>
          <def>Arm</def>
          <customLabel>right splenius</customLabel>
          <coverage>0.000001</coverage>
          <woundAnchorTag>RightShoulder</woundAnchorTag>
          <groups>
            <li>Shoulders</li>
            <li>Arms</li>
            <li>Hands</li>
            <li>RightHand</li>
          </groups>
        </li>
        <li>
          <def>Waist</def>
          <coverage>0</coverage>
          <height>Bottom</height>
          <groups>
            <li>Waist</li>
          </groups>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left trapezius</customLabel>
          <coverage>0.000001</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>Feet</li>
          </groups>
          <woundAnchorTag>LeftLeg</woundAnchorTag>
          <flipGraphic>true</flipGraphic>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right trapezius</customLabel>
          <coverage>0.000001</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>Feet</li>
          </groups>
          <woundAnchorTag>RightLeg</woundAnchorTag>
        </li> -->
      </parts>
    </corePart>
  </BodyDef>

  <BodyPartDef>
    <defName>BS_LocomotiveLung</defName>
    <label>locomotive lung</label>
    <hitPoints>25</hitPoints>
    <skinCovered>false</skinCovered>
    <tags>
      <li>BreathingSource</li>
      <li>ManipulationLimbCore</li>
      <li>MovingLimbCore</li>
      <li>Mirrored</li>
    </tags>
    <spawnThingOnRemoved>Lung</spawnThingOnRemoved>
    <modExtensions>
      <li Class="BigAndSmall.BodyPartExtension">
        <importAllRecipesFrom><li>Lung</li></importAllRecipesFrom>
      </li>
    </modExtensions>
  </BodyPartDef>

  <BodyPartDef>
    <defName>BS_AirThrusters</defName>
    <label>locomotive thrusters</label>
    <hitPoints>30</hitPoints>
    <skinCovered>false</skinCovered>
    <frostbiteVulnerability>2</frostbiteVulnerability>
    <permanentInjuryChanceFactor>0</permanentInjuryChanceFactor>
    <solid>true</solid>
    <alive>false</alive>
    <bleedRate>0</bleedRate>
    <tags>
      <li>BreathingSource</li>
      <li>ManipulationLimbCore</li>
      <li>MovingLimbCore</li>
      <li>Mirrored</li>
    </tags>
    <spawnThingOnRemoved>Lung</spawnThingOnRemoved>
    <modExtensions>
      <li Class="BigAndSmall.BodyPartExtension">
        <importAllRecipesFrom><li>Lung</li></importAllRecipesFrom>
        <mechanicalVersionOf><li>BS_LocomotiveLung</li></mechanicalVersionOf>
      </li>
    </modExtensions>
  </BodyPartDef>
  
</Defs>